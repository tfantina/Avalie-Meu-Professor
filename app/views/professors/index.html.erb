
<div class="row">
<div class="list-group">
  <ul class="list-unstyled">
<% @professors.each do |professor| %>

  <%= link_to professor, class: "list-group-item" do %>
  <h4><%= professor.try(:fullname) %></h4>
        <%= professor.try(:school) %>
        <%= professor.try(:department) %>
        <% end %>

  <% end %>
</ul>
</div>
    </div>
 <%= will_paginate @professors %>
