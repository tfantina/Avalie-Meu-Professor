<dic class="row">

  <% @professors.each do |professor| %>
    <div class="col-sm-6 col-md-3">
        <div class="thumbnail">
  <%= link_to professor, class: "list-group-item" do %>
      <center><h4><ul class="list-unstyled">
        <li><%= professor.fullname %> </li>
      </ul></h4></center>
      <ul class="list-unstyled text-muted">
        <li> <%= professor.school %></li>
        <li class="small"> <%= professor.department %></li>
      </ul>
<% end %>
      </div>
    </div>
  <% end %>
  <%= will_paginate @professors %>


</div>
